{
    "name": "moldwebs/yii2-mpdf",
    "description": "A Yii2 wrapper component for the mPDF library which generates PDF files from UTF-8 encoded HTML.",
    "keywords": [
        "yii2",
        "extension",
        "pdf",
        "mpdf",
        "utf8",
        "html",
        "component"
    ],
    "homepage": "https://github.com/moldwebs/yii2-mpdf",
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "moldwebs",
            "email": "moldwebs@gmail.com"
        }
    ],
    "require": {
        "mpdf/mpdf": "<7.0"
    },
    "autoload": {
        "psr-4": {
            "moldwebs\\mpdf\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
